<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean name="serviceEntityService"
		class="eu.aniketos.wp2.components.trustworthiness.impl.trust.service.ServiceEntityServiceImpl">
		<property name="serviceDao" ref="serviceDao"></property>
	</bean>
	
	<bean name="scoreEntityService"
		class="eu.aniketos.wp2.components.trustworthiness.impl.trust.service.ScoreEntityServiceImpl">
		<property name="scoreDao" ref="scoreDao"></property>
	</bean>
	
	<bean name="securityEntityService"
		class="eu.aniketos.wp2.components.trustworthiness.impl.trust.service.SecurityEntityServiceImpl">
		<property name="secPropertyDao" ref="secPropertyDao"></property>
	</bean>

</beans>
